{
  "name": "client",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watch --coverage=false",
    "test-ci": "jest --watch --coverage=true --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "postinstall": "patch-package",
    "build": "expo export",
    "build:web": "expo export:web"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@expo/metro-runtime": "~4.0.1",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/stack": "^7.2.3",
    "expo": "~52.0.42",
    "expo-status-bar": "~2.0.1",
    "graphql": "^16.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.8",
    "react-native-gesture-handler": "2.20.2",
    "react-native-reanimated": "3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "redux-observable": "^3.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-typescript": "^7.27.0",
    "@expo/webpack-config": "19.0.1",
    "@react-native-community/cli": "latest",
    "@react-native/babel-preset": "^0.78.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.0"
  },
  "resolutions": {
    "strip-ansi": "^7.1.0"
  },
  "jest": {
    "verbose": true,
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|@expo|expo|@unimodules|@react-navigation|@react-native-js-polyfills|react-native-vector-icons|react-native-safe-area-context|react-native-reanimated|react-native-gesture-handler|react-native-screens)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "private": true
}